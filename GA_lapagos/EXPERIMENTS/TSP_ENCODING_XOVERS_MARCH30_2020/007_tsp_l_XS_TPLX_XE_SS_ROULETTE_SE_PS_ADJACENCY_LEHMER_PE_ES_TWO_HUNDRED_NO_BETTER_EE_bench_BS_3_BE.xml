<ga_config>
	<ga_type>TSP_GA</ga_type>
	<crossover_type>TPLX</crossover_type>
	<selector_type>ROULETTE</selector_type>
	<rand_seed>1</rand_seed>
	<problem_type>ADJACENCY_LEHMER</problem_type>
	<benchmark_file_name>../../../TSP_BENCHMARKS/si1032.xml</benchmark_file_name>
	<threads>1</threads>
	<exit_type>TWO_HUNDRED_NO_BETTER</exit_type>
	<ga_params>
		<num_population>500</num_population>
		<!-- following parameters must add to 1.0 -->
		<percent_to_keep>0.01</percent_to_keep>
		<percent_from_mutate>0.00</percent_from_mutate>
		<percent_from_breed>0.00</percent_from_breed>
		<percent_from_breed_and_mutate>0.98</percent_from_breed_and_mutate>
		<percent_to_random>0.01</percent_to_random>
		<!-- the above parameters must add to 1.0 -->
		<top_percent_to_breed_from>0.10</top_percent_to_breed_from>
		<top_percent_to_mutate_from>0.10</top_percent_to_mutate_from>
		<top_percent_to_breed_mutate_from>0.10</top_percent_to_breed_mutate_from>
		<percent_of_genome_mutations>0.05</percent_of_genome_mutations>
		<!-- if tournament selection -->
		<tournament_size>8</tournament_size>
	</ga_params>
</ga_config>
